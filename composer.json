	"keywords": ["indieweb", "webmentions", "HTML", "microformats"],
	"licence": "CC0-1.0",
	"authors": [
		{
			"name": "Jonny Barnes",
			"homepage": "https://jonnybarnes.uk"
		}
	],
	"support": {
		"issues": "https://github.com/jonnybarnes/webmentions-parser/issues"
